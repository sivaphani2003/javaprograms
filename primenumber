import java.util.Scanner;
public class primenumber
{
  public static void main(String[] args)
  {
    Scanner sc= new Scanner(System.in);
    System.out.println("enter a number: ");
    int x= sc.nextInt();
    int flag=0;
    for(int i=2;i<x;i++)
    {
      if (x%i == 0)
      {
        flag=1;
        break;
      }
    }
    if (flag==1)
    {
      System.out.printf("%d number is not a prime number",x);
      System.out.println("") ;
    }
    else
    {
      System.out.printf("%d number is a prime number",x);
      System.out.println("") ;
    }
  }
} 
